<?xml version="1.0" ?>

<xsd:schema targetNamespace="urn:SpfTypes"
            xmlns:spftypes="urn:SpfTypes"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"  
            xmlns:jxb="http://java.sun.com/xml/ns/jaxb"  jxb:version="2.0">

    <xsd:import namespace="http://www.w3.org/2001/XMLSchema"/>

    <xsd:simpleType name="SpfSampleVolumeUnitEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ul"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="SpfSampleConcentrationUnitEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ng/ul"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="SpfShipFromEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="BSP"/>
            <xsd:enumeration value="Collaborator"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="SpfSampleRecepatcleGroupTypeEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="GAP Generated"/>
            <xsd:enumeration value="Pool Oligo Group"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="SpfShippingMethodEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="FedEx"/>
            <xsd:enumeration value="UPS"/>
            <xsd:enumeration value="USPS"/>
            <xsd:enumeration value="Airborne"/>
            <xsd:enumeration value="Hand Delivered"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="SpfQCOptionEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="None"/>
            <xsd:enumeration value="Pico"/>
            <xsd:enumeration value="2 Hour Gel">
            <xsd:annotation>
                <xsd:appinfo>
                    <jxb:typesafeEnumMember name="TWO_HOUR_GEL" />
                 </xsd:appinfo>
            </xsd:annotation>
            </xsd:enumeration>

            <xsd:enumeration value="Pulse Field Gel"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="SpfSectionEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ALL96"/>
            <xsd:enumeration value="ALL384"/>
            <xsd:enumeration value="A1"/>
            <xsd:enumeration value="A2"/>
            <xsd:enumeration value="B1"/>
            <xsd:enumeration value="B2"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="SpfFreezerGroupEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Core Sequencing"/>
            <xsd:enumeration value="Finishing"/>
            <xsd:enumeration value="Library Construction"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- lc_work_group.description -->
    <xsd:simpleType name="SpfWorkGroupEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Cancer Resequencing"/>
            <xsd:enumeration value="Dog Genome"/>
            <xsd:enumeration value="HT-PCR sequencing"/>
            <xsd:enumeration value="Library Construction"/>
            <xsd:enumeration value="Next Generation Sequencing"/>
            <xsd:enumeration value="Viral Sequencing"/>
        </xsd:restriction>
    </xsd:simpleType>
    <!--place holder for more data mapping -->

    <xsd:complexType name="SpfNameValuePair">
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string"/>
            <xsd:element name="value" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <!-- plate_physical_type.name -->
    <xsd:simpleType name="SpfPhysicalPlateEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="PureDNA"/>
            <!-- name="PureDNA1" -->
            <xsd:enumeration value="PCR96"/>
            <xsd:enumeration value="GreinerDeepWell384"/>
            <xsd:enumeration value="FalconTube"/>
            <xsd:enumeration value="RoundBottom96"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- lc_sample_receptacle_type.name -->
    <xsd:simpleType name="SpfReceptacleTypeEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="96 Well Plate">
            <xsd:annotation>
                <xsd:appinfo>
                    <jxb:typesafeEnumMember name="NINETYSIX_WELL_PLATE" />
                 </xsd:appinfo>
            </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Bag"/>
            <xsd:enumeration value="Box"/>
            <xsd:enumeration value="Large Tube"/>
            <xsd:enumeration value="Medium Tube"/>
            <xsd:enumeration value="Plate"/>
            <xsd:enumeration value="Small Tube"/>
            <xsd:enumeration value="2D Tube">
            <xsd:annotation>
                <xsd:appinfo>
                    <jxb:typesafeEnumMember name="TWO_D_TUBE" />
                 </xsd:appinfo>
            </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2D Medium Tube">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:typesafeEnumMember name="TWO_D_MEDIUM_TUBE" />
                     </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2D xSmall Tube">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:typesafeEnumMember name="TWO_D_XSMALLTUBE" />
                     </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Unspecified"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- lc_sample_receptacle_status.name -->
    <xsd:simpleType name="SpfReceptacleStatusEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Empty"/>
            <xsd:enumeration value="Received"/>
            <xsd:enumeration value="QC Complete"/>
            <xsd:enumeration value="Not Received"/>
            <xsd:enumeration value="Handed off to outside lab"/>
            <xsd:enumeration value="DNA Extracted"/>
            <xsd:enumeration value="Destroyed"/>
            <xsd:enumeration value="Shipped"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- lc_sample_status.name -->
    <xsd:simpleType name="SpfSampleStatusEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Consumed"/>
            <xsd:enumeration value="Consumed - Container destroyed"/>
            <xsd:enumeration value="DNA Extracted"/>
            <xsd:enumeration value="Destroyed/Discarded"/>
            <xsd:enumeration value="Empty"/>
            <xsd:enumeration value="Handed off to outside lab"/>
            <xsd:enumeration value="Missing"/>
            <xsd:enumeration value="QC Complete"/>
            <xsd:enumeration value="Received"/>
            <xsd:enumeration value="Registered"/>
            <xsd:enumeration value="Restricted - Do Not Distribute"/>
            <xsd:enumeration value="Returned to GSSR"/>
            <xsd:enumeration value="Temporarily removed for processing"/>
            <xsd:enumeration value="Wait For QC"/>
            <xsd:enumeration value="Virtual"/>
            <xsd:enumeration value="Re-registered in BSP"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="SpfMaterialTypeEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="BAC/Fosmid Library"/>
            <xsd:enumeration value="Genomic DNA"/>
            <xsd:enumeration value="Glycerol clone"/>
            <xsd:enumeration value="PCR Product"/>
            <xsd:enumeration value="Primer"/>
            <xsd:enumeration value="Tissue"/>
            <xsd:enumeration value="Viral cDNA"/>
            <xsd:enumeration value="Voucher"/>
            <xsd:enumeration value="cDNA Library"/>
            <xsd:enumeration value="sstDNA library"/>
            <xsd:enumeration value="New Tech Library"/>
            <xsd:enumeration value="HybSelection Pool Oligo"/>
            <xsd:enumeration value="Other"/>
            <xsd:enumeration value="RNA"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- genomic_material_format.material_format_name -->
    <xsd:simpleType name="SpfMaterialFormatEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="0.5ml screwcap tube">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:typesafeEnumMember name="ZERO_POINT_FIVE_ML_SCREWCAP_TUBE" />
                     </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>

            <xsd:enumeration value="1.5ml Nalgene cryotube - screwcap">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:typesafeEnumMember name="ONE_POINT_FIVE_ML_NALGENE_CYRO_TUBE_SCREWCAP" />
                     </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>

            <xsd:enumeration value="1.5ml eppi tube - fliptop">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:typesafeEnumMember name="ONE_POINT_FIVE_ML_EPPI_TUBE_FLIPTOP" />
                     </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>

            <xsd:enumeration value="15ml Falcon Tube">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:typesafeEnumMember name="FIFTEEN_ML_FALCON_TUBE" />
                     </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>

            <xsd:enumeration value="2.0ml Nunc cryotube - screwcap">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:typesafeEnumMember name="TWO_ML_NUNC_CYRO_TUBE_SCREWCAP" />
                     </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>

            <xsd:enumeration value="2.0ml Nunc roundbottom cryotube - screwcap internal thread">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:typesafeEnumMember name="TWO_ML_NUNC_RB_CYRO_TUBE_SCREWCAP" />
                     </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>

            <xsd:enumeration value="50ml Costar Tube">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:typesafeEnumMember name="FIFTY_ML_COSTAR_TUBE" />
                     </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>

            <xsd:enumeration value="50ml Falcon Tube - freestanding">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:typesafeEnumMember name="FIFTY_ML_FALCON_TUBE_FREESTANDING" />
                     </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>

            <xsd:enumeration value="50ml Falcon Tube">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:typesafeEnumMember name="FIFTY_ML_FALCON_TUBE" />
                     </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>

            <xsd:enumeration value="Agar Plates"/>
            <xsd:enumeration value="Bag in box"/>
            <xsd:enumeration value="Bag"/>
            <xsd:enumeration value="Bags in boxes"/>
            <xsd:enumeration value="Boxes"/>
            <xsd:enumeration value="Packet"/>
            <xsd:enumeration value="Plate - 384 Well"/>
            <xsd:enumeration value="Plate - 96 Well"/>
            <xsd:enumeration value="Tubes with stab cultures"/>
            <xsd:enumeration value="Ziploc"/>
            <xsd:enumeration value="vacutainer tube"/>
            <xsd:enumeration value="0.5ml 2D Barcoded Tube">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:typesafeEnumMember name="ZERO_POINT_FIVE_ML_2D_BARCODED_TUBE" />
                     </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>

            <xsd:enumeration value="1.0ml 2D Barcoded Tube">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:typesafeEnumMember name="ONE_ML_2D_BARCODED_TUBE" />
                     </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>

            <xsd:enumeration value="10ml 2D Barcoded Tube">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:typesafeEnumMember name="TEN_ML_2D_BARCODED_TUBE" />
                     </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>

            <xsd:enumeration value="Unspecified"/>
        </xsd:restriction>
    </xsd:simpleType>


    <!-- genomic_material_suspension.material_suspension_name -->
    <xsd:simpleType name="SpfMaterialSuspensionEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="agarose plugs"/>
            <xsd:enumeration value="dried pellets"/>
            <xsd:enumeration value="frozen"/>
            <xsd:enumeration value="glycerol"/>
            <xsd:enumeration value="ligation"/>
            <xsd:enumeration value="precip in alcohol or salt"/>
            <xsd:enumeration value="unspecified"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- genomic_sample_organism.sex -->
    <xsd:simpleType name="SpfOrganismSexEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="M"/>
            <xsd:enumeration value="F"/>
            <xsd:enumeration value="O"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- lc_sample_receptacle_location.location -->
    <xsd:simpleType name="SpfReceptacleLocationEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="+4 Freezer">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:typesafeEnumMember name="PLUS_4_FREEZER" />
                     </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>

            <xsd:enumeration value="-20 Freezer">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:typesafeEnumMember name="MINUS_20_FREEZER" />
                     </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>

            <xsd:enumeration value="-80 Freezer">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:typesafeEnumMember name="MINUS_80_FREEZER" />
                     </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>

            <xsd:enumeration value="Outside Repository"/>
            <xsd:enumeration value="Unknown"/>
        </xsd:restriction>
    </xsd:simpleType>

