    <xsd:simpleType name="SpfSampleTypeEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Positive Control"/>
            <xsd:enumeration value="Negative Control"/>
            <xsd:enumeration value="BSP Sample"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- entry_plate -->
    <xsd:complexType name="SpfPlate">
        <xsd:sequence>
            <xsd:element name="plateSection" type="spftypes:SpfSectionEnum"/>
            <!-- entry_plate.barcode -->
            <xsd:element name="plateBarcode" type="xsd:string"/>
            <xsd:element name="platePhysical"
                         type="spftypes:SpfPhysicalPlateEnum"/>
            <!-- station_event.comments -->
            <xsd:element name="plateComment" type="xsd:string" maxOccurs="1"
                         minOccurs="0"/>
        </xsd:sequence>

    </xsd:complexType>

    <!-- This is only used internally for all bettalims plate registrations -->
    <xsd:complexType name="SpfPlateRegRequest">
        <xsd:sequence>
            <!-- sequencing_library.library_name -->
            <xsd:element name="plateSeqLibrary" type="xsd:string"
                         maxOccurs="1"
                         minOccurs="0"/>
            <xsd:element name="registrationPlate" type="spftypes:SpfPlate"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- genomic_sample_material -->
    <xsd:complexType name="SpfSampleMaterial">
        <xsd:sequence>

            <!-- genomic_material_type.material_type_name -->
            <xsd:element name="materialType"
                         type="spftypes:SpfMaterialTypeEnum"/>

            <!-- genomic_material_format.material_format_name -->
            <xsd:element name="materialFormat"
                         type="spftypes:SpfMaterialFormatEnum"/>

            <!-- genomic_material_suspension.material_suspension_name -->
            <xsd:element name="materialSuspension"
                         type="spftypes:SpfMaterialSuspensionEnum"/>

        </xsd:sequence>
    </xsd:complexType>


    <!-- genomic_sample_organism -->
    <xsd:complexType name="SpfSampleOrganism">
        <xsd:sequence>

            <!-- genomic_sample_organism.organism -->
            <xsd:element name="orgName" type="xsd:string"/>

            <!-- genomic_sample_organism.individual_name -->
            <xsd:element name="orgIndividualName" type="xsd:string"/>

            <!-- genomic_sample_organism.sex -->
            <xsd:element name="OrganismSex"
                         type="spftypes:SpfOrganismSexEnum"
                         maxOccurs="1" minOccurs="0"/>

            <!-- genomic_sample_organism.strain -->
            <xsd:element name="strain" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>

            <!-- genomic_sample_organism.external_identifier -->
            <xsd:element name="orgMatingType" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>

            <!-- genomic_sample_organism.cell_line -->
            <xsd:element name="orgCellLine" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>

            <!-- genomic_sample_organism.method_of_immortalization -->
            <xsd:element name="orgMethOfImmortalization"
                         type="xsd:string" maxOccurs="1" minOccurs="0"/>

            <!-- genomic_sample_organism.external_identifier -->
            <xsd:element name="orgExternalID" type="xsd:string"/>

            <!-- genomic_sample_organism.irb_number -->
            <xsd:element name="orgIrbNumber" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>

            <!-- genomic_sample_organism.other_description -->
            <xsd:element name="otherDescription" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>

    </xsd:complexType>

    <xsd:complexType name="SpfSampleOrganismAttributes">
        <xsd:sequence>
            <xsd:element name="LSId" type="xsd:string" maxOccurs="1"
                         minOccurs="0"/>
            <xsd:element name="BCR_GSC_Full_Biospecimen_ID" type="xsd:string" maxOccurs="1"
                         minOccurs="0"/>
            <xsd:element name="externalID" type="xsd:string"/>
            <xsd:element name="individualName" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="strain" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="sex"
                         type="spftypes:SpfOrganismSexEnum"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="matingType" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="cellLine" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="irbNumber" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="otherOrganismDescription" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>


    </xsd:complexType>

    <!-- lc_sample_amount_data -->
    <xsd:complexType name="SpfSampleAmountData">
        <xsd:sequence>
            <!-- lc_sample_amount_data.amount_measure_unit -->
            <xsd:element name="amountMeasureUnit" type="spftypes:SpfSampleVolumeUnitEnum"/>
            <!-- lc_sample_amount_data.initial_amount -->
            <xsd:element name="amountInitial" type="xsd:double"/>

            <!-- lc_sample_amount_data.remaining_amount -->
            <xsd:element name="amountRemaining" type="xsd:double"/>

            <!-- lc_sample_amount_data.concentration -->
            <xsd:element name="amountConcentration" type="xsd:double"
                         maxOccurs="1" minOccurs="0"/>
            <!-- lc_sample_amount_data.concentration -->
            <xsd:element name="concentrationUnit" type="spftypes:SpfSampleConcentrationUnitEnum"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- lc_sample_amount_data -->
    <xsd:complexType name="SpfSampleAmount">
        <xsd:sequence>
            <!-- lc_sample_amount_data.amount_measure_unit -->
            <xsd:element name="amountMeasureUnit" type="xsd:string"/>

            <!-- lc_sample_amount_data.initial_amount -->
            <xsd:element name="amountInitial" type="xsd:double"/>

            <!-- lc_sample_amount_data.remaining_amount -->
            <xsd:element name="amountRemaining" type="xsd:double"/>

            <!-- lc_sample_amount_data.concentration -->
            <xsd:element name="amountConcentration" type="xsd:double"
                         maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>


    <!-- lc_sample_genome_data -->
    <xsd:complexType name="SpfSampleGenome">
        <xsd:sequence>
            <!-- lc_sample_genome_data.fragment_size -->
            <xsd:element name="genomeFragmentSize"
                         type="xsd:string" maxOccurs="1" minOccurs="0"/>

            <!-- lc_sample_genome_data.tissue_source -->
            <xsd:element name="genomeTissueSource"
                         type="xsd:string" maxOccurs="1" minOccurs="0"/>

            <!-- lc_sample_genome_data.buffer -->
            <xsd:element name="genomeBuffer" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>

            <!-- lc_sample_genome_data.dna_extraction_method -->
            <xsd:element name="genomeExtractionMethod"
                         type="xsd:string" maxOccurs="1" minOccurs="0"/>
            <xsd:element name="sampleSpike" type="xsd:string" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SpfGenomicDNASampleData">
        <xsd:sequence>
            <!-- lc_sample_genome_data.fragment_size -->
            <xsd:element name="fragmentSize"
                         type="xsd:string" maxOccurs="1" minOccurs="0"/>

            <!-- lc_sample_genome_data.buffer -->
            <xsd:element name="buffer" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>

            <!-- lc_sample_genome_data.dna_extraction_method -->
            <xsd:element name="extractionMethod"
                         type="xsd:string" maxOccurs="1" minOccurs="0"/>
            <xsd:element name="sampleSpike"
                         type="xsd:string" maxOccurs="1" minOccurs="0"/>

        </xsd:sequence>
    </xsd:complexType>

    <!-- lc_sample_pcr_data -->
    <xsd:complexType name="SpfSamplePCR">
        <xsd:sequence>
            <!-- lc_sample_pcr_data.forward_primer_name -->
            <xsd:element name="pcrForwardPrimerName" type="xsd:string"/>

            <!-- lc_sample_pcr_data.forward_primer
               required by NCBI rfc: AMPLIFICATION_FORWARD -->
            <xsd:element name="pcrForwardPrimerSequence" type="xsd:string"/>

            <!-- lc_sample_pcr_data.reverse_primer_name -->
            <xsd:element name="pcrReversePrimerName" type="xsd:string"/>

            <!-- lc_sample_pcr_data.reverse_primer
               required by NCBI rfc: AMPLIFICATION_REVERSE -->
            <xsd:element name="pcrReversePrimerSequence" type="xsd:string"/>

            <!-- lc_sample_pcr_data.pooled_pcr -->
            <xsd:element name="pcrPooled" type="xsd:boolean"/>

            <!-- lc_sample_pcr_data.product_size
               optional by NCBI rfc: AMPLIFICATION_SIZE -->
            <xsd:element name="pcrProductSize" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>

            <!-- lc_sample_pcr_data.reference_accession -->
            <!-- required by NCBI rfc: REFERENCE_ACCESSION -->
            <xsd:element name="pcrReferenceAccession" type="xsd:string"/>

            <!-- lc_sample_pcr_data.reference_accession_max -->
            <!-- NCBI rfc: REFERENCE_ACC_MAX -->
            <xsd:element name="pcrReferenceAccessionMax" type="xsd:long"
                         maxOccurs="1" minOccurs="0"/>

            <!-- lc_sample_pcr_data.reference_accession_mim -->
            <!-- NCBI rfc: REFERENCE_ACC_MIN -->
            <xsd:element name="pcrReferenceAccessionMin" type="xsd:long"
                         maxOccurs="1" minOccurs="0"/>

            <!-- lc_sample_pcr_data.exon_number -->
            <xsd:element name="pcrExonNumber" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>

            <!-- lc_sample_pcr_data.gene_name -->
            <xsd:element name="pcrGeneName" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>

            <xsd:element name="primerMolBarcodeName" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="primerMolBarcodeSeq" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>


        </xsd:sequence>
    </xsd:complexType>

    <!-- lc_sample_viral_data.single_or_double_stranded -->
    <xsd:simpleType name="SpfViralDataStrand">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="S"/>
            <xsd:enumeration value="D"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- lc_sample_viral_data -->
    <xsd:complexType name="SpfSampleViral">
        <xsd:sequence>
            <!-- lc_sample_viral_data.serotype -->
            <xsd:element name="serotype" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>

            <!-- lc_sample_viral_data.viral_host -->
            <xsd:element name="viralHost" type="xsd:string"/>

            <!-- lc_sample_viral_data.population_id -->
            <xsd:element name="populationId" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>

            <!-- lc_sample_viral_data.concentration -->
            <xsd:element name="intendedSeqMethod" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>

            <!-- lc_sample_viral_data.single_or_double_stranded -->
            <xsd:element name="singleOrDoubleStranded" type="spftypes:SpfViralDataStrand"
                         maxOccurs="1" minOccurs="0"/>

        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SpfViralcDNASampleData">
        <xsd:sequence>
            <xsd:element name="serotype" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="viralHost" type="xsd:string"/>
            <xsd:element name="sampleCohortPopulation" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="intendedSeqMethod" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="passageHistory" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="singleOrDoubleStranded" type="spftypes:SpfViralDataStrand"
                         maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SpfNewTechLibData">
        <xsd:sequence>
            <xsd:element name="sequencingTechnology" type="xsd:string"/>
            <!--IPI-57758 -->
            <xsd:element name="dataAnalysisType" type="xsd:string"
                     maxOccurs="1" minOccurs="0"/>
            <xsd:element name="restrictionEnzyme" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="libraryType" type="xsd:string"/>
            <xsd:element name="adapterTypeAndName" type="xsd:string"/>
            <xsd:element name="insertSizeRange" type="xsd:string"/>
            <xsd:element name="librarySizeRange" type="xsd:string"/>
            <xsd:element name="jumpSize" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="molBarName" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="molBarSeq" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
             <xsd:element name="HybBaitDesignName" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>

        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="SpfPoolOligoData">
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string"/>
            <xsd:element name="referenceSequenceAlias" type="xsd:string"/>
            <xsd:element name="referenceSequenceVersion" type="xsd:int"
                         minOccurs="0"/>
            <xsd:element name="targetSetName" type="xsd:string" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="SpfSampleReceptacleGroupData">
        <xsd:sequence>
            <xsd:element name="groupType" type="spftypes:SpfSampleRecepatcleGroupTypeEnum"/>
            <xsd:element name="groupName" type="xsd:string"/>
            <xsd:element name="referenceSequenceAlias" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="SpfSampleCollectionDate">
        <xsd:sequence>
            <xsd:element name="dd" type="xsd:string"/>
            <xsd:element name="mm" type="xsd:string"/>
            <xsd:element name="yyyy" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SpfSampleCollectionData">
        <xsd:sequence>
            <xsd:element name="collectionDate" type="spftypes:SpfSampleCollectionDate"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="country" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="city" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="stateRegion" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="hostId" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="hostOrganism" type="spftypes:SpfOrganism"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="hostLocation" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SpfProjectCreationData">
        <xsd:sequence>
            <xsd:element name="initiativeName" type="xsd:string"/>
            <xsd:element name="fundingSource" type="xsd:string"/>
            <xsd:element name="projectComment" type="xsd:string" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="SpfCreateProjectEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Yes"/>
            <xsd:enumeration value="No"/>
        </xsd:restriction>
    </xsd:simpleType>
    <!-- lc_sample_collection_data -->
    <xsd:complexType name="SpfSampleCollection">
        <xsd:sequence>
            <!-- lc_sample_collection_data.collection_date -->
            <xsd:element name="collectionDate" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>

            <!-- lc_sample_collection_data.country -->
            <xsd:element name="country" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>

            <!-- lc_sample_collection_data.city -->
            <xsd:element name="city" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>

            <!-- lc_sample_collection_data.state_region -->
            <xsd:element name="stateRegion" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="hostId" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="hostOrganism" type="spftypes:SpfOrganism"
                         maxOccurs="1" minOccurs="0"/>
            <xsd:element name="hostLocation" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>

        </xsd:sequence>
    </xsd:complexType>
    
    <!-- LCT-363 -->
    <xsd:complexType name="SampleKitCollaboratorWithOrg">
        <xsd:sequence>
            <xsd:element name="firstName"
                         type="xsd:string"/>
            <xsd:element name="lastName"
                         type="xsd:string"/>
            <xsd:element name="organization"
                         type="xsd:string" maxOccurs="1"
                         minOccurs="0"/>
            <xsd:element name="gssrCollaboratorID"
                         type="xsd:long" maxOccurs="1"
                         minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <!-- LCT-363 end-->

    <!-- Organism -->
    <xsd:complexType name="SpfOrganism">
        <xsd:sequence>
            <xsd:element name="genus" type="xsd:string"
                         maxOccurs="1" minOccurs="1"/>
            <xsd:element name="species" type="xsd:string"
                         maxOccurs="1" minOccurs="1"/>
                <xsd:element name="commonName" type="xsd:string"
                         maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- object to pass/return a list of Sample LSID's -->
    <xsd:complexType name="SpfSampleLSID">
        <xsd:sequence>
            <xsd:element name="gssrSampleLSID" type="xsd:string" minOccurs="1" maxOccurs="1000"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="OrganismTaxon">
        <xsd:sequence>
            <xsd:element name="species" type="spftypes:SpfOrganism"/>
            <xsd:element name="strain" type="xsd:string" maxOccurs="unbounded" minOccurs="0"/>
            <xsd:element name="serotype" type="xsd:string" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="SpfExtraIdEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="UNKOWN"/>
            <xsd:enumeration value="Dog Genome Pedigree LSID"/>
            <xsd:enumeration value="BSP Sample ID"/>
            <xsd:enumeration value="Kinome Sample Instance ID"/>
            <xsd:enumeration value="BCR GSC Full Biospecimen ID"/>
            <xsd:enumeration value="TCGA UUID"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="SpfExtraIdValuePair">
        <xsd:sequence>
            <xsd:element name="name" type="spftypes:SpfExtraIdEnum"/>
            <xsd:element name="value" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="SpfChildParentSampleMap">
        <xsd:sequence>
            <xsd:element name="childLsid" type="xsd:string"></xsd:element>
            <xsd:element name="parentLsid" type="xsd:string" maxOccurs="unbounded" minOccurs="1"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    
    <xsd:complexType name="SpfInitiative">
      <xsd:annotation>
        <xsd:documentation>
          Currently modeling the only two attributes of an initiative that we care about
          to satisfy INF-446
        </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
        <xsd:element name="name" type="xsd:string" minOccurs="1" maxOccurs="1"/>
        <xsd:element name="protected" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
      </xsd:sequence>
    </xsd:complexType>
    
    
    <xsd:complexType name="SpfInitiativeListResult">
      <xsd:sequence>
        <xsd:element name="initiativeList" minOccurs="0" maxOccurs="unbounded" type="spftypes:SpfInitiative" />
      </xsd:sequence>
    </xsd:complexType>    
</xsd:schema>
